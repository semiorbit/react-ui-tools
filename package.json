{
  "name": "@semiorbit/react-ui-tools",
  "version": "2.0.9",
  "description": "React-js Material-ui controls wrapper and other custom controls",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semiorbit/react-ui-tools.git"
  },
  "author": "Ibrahim Khalil Khalbous",
  "license": "MIT",
  "files": [
    "./lib"
  ],
  "keywords": [
    "Alert",
    "useAlert",
    "Alert Hook",
    "material-ui"
  ],
  "scripts": {
    "build": "npm run v && npm i  --legacy-peer-deps --save && rollup -c",
    "build-watch": "rollup -c -w",
    "start-playground": "cd playground && npm run start",
    "i-all": "npm i && cd playground && npm i",
    "dev": "npm-run-all --parallel build-watch",
    "v": "npm version patch --force && cd lib && npm version patch --force",
    "publish": "npm publish lib --access public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "npm-run-all": "^4.1.5",
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@rollup/plugin-babel": "^6.0.3",
    "react-scripts": "5.0.1",
    "rollup": "^3.29.4",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "babel-plugin-module-resolver": "^5.0.0"
  },
  "peerDependencies": {
    "@mui/material": ">=5.10.11",
    "@semiorbit/is-iphonex": ">=1.0.2",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-i18next": ">=13.2.2",
    "@semiorbit/react-form-provider": ">=1.1.31"
  }
}
