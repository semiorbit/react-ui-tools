{
  "name": "@semiorbit/react-ui-tools",
  "version": "1.3.6",
  "description": "React-js Material-ui controls wrapper and other custom controls",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semiorbit/react-ui-tools.git"
  },
  "author": "Ibrahim Khalil Khalbous",
  "license": "MIT",
  "files": [
    "./lib"
  ],
  "scripts": {
    "build": "npm run v && npm i && rollup -c",
    "build-watch": "rollup -c -w",
    "start-playground": "cd playground && npm run start",
    "i-all": "npm i && cd playground && npm i",
    "dev": "npm-run-all --parallel build-watch",
    "v": "npm version patch --force && cd lib && npm version patch --force",
    "publish": "npm publish lib --access public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "react-scripts": "4.0.3",
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.44.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "jss": "^10.6.0",
    "jss-rtl": "^0.3.0",
    "react-i18next": "^11.8.12",
    "web-vitals": "^1.1.1"
  }
}
